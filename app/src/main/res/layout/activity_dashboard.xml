<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Dashboard">

    <EditText
        android:id="@+id/editTextTextMultiLine3"
        android:layout_width="162dp"
        android:layout_height="67dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="484dp"
        android:background="@drawable/my_border"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="12kg Refill"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/searchView" />

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="362dp"
        android:layout_height="40dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/my_border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="66dp"
        android:layout_height="121dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="96dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        app:srcCompat="@drawable/cylinder" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="66dp"
        android:layout_height="121dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="272dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        app:srcCompat="@drawable/cylinder" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="66dp"
        android:layout_height="121dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="452dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        app:srcCompat="@drawable/cylinder" />

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="162dp"
        android:layout_height="67dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/my_border"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="@string/item"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/searchView" />

    <EditText
        android:id="@+id/editTextTextMultiLine2"
        android:layout_width="162dp"
        android:layout_height="67dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="296dp"
        android:background="@drawable/my_border"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="3kg Refill"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/searchView" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/bottom_home_navigation"
        android:layout_marginTop="650dp"

        >

        <ImageButton
            android:id="@+id/bottom_home_navigation"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:src="@drawable/ic_baseline_home_24"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="53dp"
            tools:layout_editor_absoluteY="583dp" />

<!--        <ImageButton-->
<!--            android:id="@+id/bottom_gas_navigation"-->
<!--            android:layout_width="50dp"-->
<!--            android:layout_height="50dp"-->
<!--            android:layout_marginLeft="150dp"-->
<!--            android:src="@drawable/gas_icon"-->
<!--            tools:ignore="MissingConstraints"-->
<!--            tools:layout_editor_absoluteX="53dp"-->
<!--            tools:layout_editor_absoluteY="583dp" />-->

        <ImageButton
            android:id="@+id/bottom_profile_navigation"
            android:onClick="sendMessageToProfile"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="340dp"
            android:src="@drawable/ic_baseline_person_24"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="53dp"
            tools:layout_editor_absoluteY="583dp" />

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>